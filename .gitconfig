[user]
        name = Cesar Figueroa
        email = cesar.figueroa@me.com
[color]
        ui = true
[core]
        excludesfile = ~/.gitignore
        editor = vim
	pager = less
[alias]
        a = add
        ap = add --patch
        au = add --update
        amend = commit --amend
        assume = update-index --assume-unchanged
        noassume = update-index --no-assume-unchanged
        b = branch
        c = clone
        cl = clone --depth=1
        cm = commit -m
        co = checkout
        cp = cherry-pick
        d = diff
        ds = diff --staged
        dsw = diff --staged --color-words
        dw = diff --color-words
        h = help
        l = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %Cblue<%an>%Creset' --abbrev-commit
        p = push
        pl = pull --ff-only
        r = remote --verbose
        ra = rebase --abort
        rc = rebase --continue
        rl = reflog
        rr = remote remove
        s = status --untracked-files
        u = reset HEAD
        undo = reset --soft HEAD^
        wd = diff --word-diff
        ws = diff --check
[credential]
	helper = osxkeychain
[help]
	autocorrect = 0
[push]
	default = simple
